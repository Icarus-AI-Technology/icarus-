{
  "name": "icarus-make",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Neuomorphic Screen Design - Imported from Figma Make",
  "scripts": {
    "qa:a11y:pw": "node scripts/qa/a11y-playwright-check.mjs",
    "qa:lh:xvfb": "node scripts/qa/lighthouse-xvfb.cjs",
    "qa:hardgates": "node scripts/qa/validate-hard-gates.mjs",
    "qa:a11y": "npx -y @axe-core/cli http://localhost:4173/ --tags wcag2a,wcag2aa --save ./docs/axe-root.json",
    "qa:perf": "npx -y lighthouse http://localhost:4173/ --preset=desktop --output=json --output-path=./docs/lh-root.json",
    "qa:ds": "npm run qa:hardgates",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "type-check": "tsc --noEmit -p tsconfig.typecheck.json",
    "validate:all": "npm run type-check && npm run lint && npm run build",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest",
    "test:unit": "vitest run src/**/*.test.ts",
    "test:integration": "vitest run src/test/integration",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "db:migrate": "bash scripts/db/migrate.sh",
    "db:seed": "bash scripts/db/seed.sh",
    "db:audit": "bash scripts/db/audit.sh",
    "db:health": "bash scripts/db/health-check-db.sh",
    "db:backup": "bash scripts/db/backup-daily.sh",
    "db:backup:setup": "bash scripts/db/setup-backup-cron.sh",
    "db:restore": "bash scripts/db/restore-backup.sh",
    "db:setup-dpo": "node scripts/db/setup-dpo-interactive.cjs",
    "db:setup-dpo:bash": "bash scripts/db/setup-dpo.sh",
    "db:deploy": "node scripts/db/deploy-supabase.mjs",
    "db:check-storage": "node scripts/db/check-storage.cjs",
    "db:gen:types": "supabase gen types typescript --local > src/types/database.types.ts",
    "preview:start": "vite preview --host --port 4173",
    "preview:capture": "node tools/design/capture-previews.js",
    "preview:setup": "npx pm2 start ecosystem.preview.config.js",
    "preview:stop": "npx pm2 stop ecosystem.preview.config.js",
    "preview:restart": "npx pm2 restart ecosystem.preview.config.js",
    "preview:logs": "npx pm2 logs icarus-preview-capture",
    "preview:logs:server": "npx pm2 logs icarus-preview-server",
    "preview:monit": "npx pm2 monit",
    "preview:delete": "npx pm2 delete ecosystem.preview.config.js",
    "infra:audit": "node tools/infra/audit.js",
    "infra:plan": "node tools/infra/plan.js",
    "infra:apply": "node tools/infra/apply.js",
    "infra:health": "node tools/infra/health.js",
    "cost:report": "node tools/ops/cost-report.js",
    "perf:sql:top": "node tools/db/sql-top.js",
    "search:reindex": "node tools/search/reindex-meili.js",
    "kpi:refresh": "node tools/db/refresh-kpis.js",
    "ai:tutor:reindex": "node tools/ai/reindex-docs.js",
    "db:validate": "node tools/db/validate-migration.js"
  },
  "dependencies": {
    "@azure/msal-browser": "^4.25.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@microsoft/microsoft-graph-types": "^2.43.1",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/react": "^10.20.0",
    "@supabase/supabase-js": "^2.75.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/blob": "^2.0.0",
    "@vercel/edge-config": "^1.4.0",
    "@vercel/kv": "^3.0.0",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.436.0",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.65.0",
    "react-router-dom": "^6.26.0",
    "recharts": "^3.3.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "tesseract.js": "^6.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@storybook/addon-a11y": "^9.1.13",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^9.1.13",
    "@storybook/react": "^9.1.13",
    "@storybook/react-vite": "^9.1.13",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.18.11",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.10.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^16.4.0",
    "globby": "^13.2.2",
    "jsdom": "^27.0.1",
    "meilisearch": "^0.53.0",
    "pg": "^8.16.3",
    "playwright": "^1.56.1",
    "pm2": "^6.0.13",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "puppeteer": "^24.25.0",
    "storybook": "^9.1.13",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.46.1",
    "vite": "^5.4.4",
    "vitest": "^3.2.4"
  }
}
