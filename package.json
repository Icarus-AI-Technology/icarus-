{
  "name": "icarus-make",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Neuomorphic Screen Design - Imported from Figma Make",
  "scripts": {
    "qa:a11y:pw": "node scripts/qa/a11y-playwright-check.mjs",
    "qa:lh:xvfb": "node scripts/qa/lighthouse-xvfb.cjs",
    "qa:hardgates": "node scripts/qa/validate-hard-gates.mjs",
    "qa:a11y": "npx -y @axe-core/cli http://localhost:4173/ --tags wcag2a,wcag2aa --save ./docs/axe-root.json",
    "qa:perf": "npx -y lighthouse http://localhost:4173/ --preset=desktop --output=json --output-path=./docs/lh-root.json",
    "qa:perf:qa": "npx -y lighthouse \"http://localhost:4173/qa/financeiro?qa=1\" --preset=desktop --output=json --output-path=./docs/lh-qa-financeiro.json",
    "qa:ds": "npm run qa:hardgates",
    "dev": "vite",
    "dev:server": "tsx watch server/index.ts",
    "dev:full": "concurrently \"pnpm dev\" \"pnpm dev:server\"",
    "build": "vite build",
    "preview": "vite preview --port 5173",
    "lint": "eslint .",
    "type-check": "tsc --noEmit -p tsconfig.typecheck.json",
    "validate:all": "npm run type-check && npm run lint && npm run build",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest",
    "test:unit": "vitest run src/**/*.test.ts",
    "test:integration": "vitest run src/test/integration",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "storybook": "STORYBOOK_DISABLE_TELEMETRY=1 storybook dev -p 6007",
    "build-storybook": "storybook build",
    "db:migrate": "bash scripts/db/migrate.sh",
    "db:seed": "bash scripts/db/seed.sh",
    "db:audit": "bash scripts/db/audit.sh",
    "db:health": "bash scripts/db/health-check-db.sh",
    "db:backup": "bash scripts/db/backup-daily.sh",
    "db:backup:setup": "bash scripts/db/setup-backup-cron.sh",
    "db:restore": "bash scripts/db/restore-backup.sh",
    "db:setup-dpo": "node scripts/db/setup-dpo-interactive.cjs",
    "db:setup-dpo:bash": "bash scripts/db/setup-dpo.sh",
    "db:deploy": "node scripts/db/deploy-supabase.mjs",
    "db:check-storage": "node scripts/db/check-storage.cjs",
    "db:gen:types": "supabase gen types typescript --local > src/types/database.types.ts",
    "preview:start": "vite preview --host --port 4173",
    "preview:capture": "node tools/design/capture-previews.js",
    "preview:setup": "npx pm2 start ecosystem.preview.config.js",
    "preview:stop": "npx pm2 stop ecosystem.preview.config.js",
    "preview:restart": "npx pm2 restart ecosystem.preview.config.js",
    "preview:logs": "npx pm2 logs icarus-preview-capture",
    "preview:logs:server": "npx pm2 logs icarus-preview-server",
    "preview:monit": "npx pm2 monit",
    "preview:delete": "npx pm2 delete ecosystem.preview.config.js",
    "infra:audit": "node tools/infra/audit.js",
    "infra:plan": "node tools/infra/plan.js",
    "infra:apply": "node tools/infra/apply.js",
    "infra:health": "node tools/infra/health.js",
    "cost:report": "node tools/ops/cost-report.js",
    "perf:sql:top": "node tools/db/sql-top.js",
    "search:reindex": "node tools/search/reindex-meili.js",
    "kpi:refresh": "node tools/db/refresh-kpis.js",
    "ai:tutor:reindex": "node tools/ai/reindex-docs.js",
    "db:validate": "node tools/db/validate-migration.js",
    "bench:tesseract": "node tools/bench/tesseract.js",
    "bench:tesseract:strict": "node tools/bench/tesseract-strict.js",
    "qa:forms": "node tools/qa/check-forms.js",
    "qa:buttons": "node tools/qa/check-buttons.js",
    "qa:tables": "node tools/qa/check-tables.js",
    "qa:integrations": "npm-run-all -s qa:meili qa:tesseract qa:ollama qa:email qa:bull qa:posthog",
    "qa:meili": "node tools/qa/integrations/check-meili.js",
    "qa:tesseract": "node tools/qa/integrations/check-tesseract.js",
    "qa:ollama": "node tools/qa/integrations/check-ollama.js",
    "qa:email": "node tools/qa/integrations/check-email.js",
    "qa:bull": "node tools/qa/integrations/check-bullmq.js",
    "qa:posthog": "node tools/qa/integrations/check-posthog.js",
    "qa:buttons:topbar": "node tools/qa/check-buttons-topbar.cjs",
    "qa:ui": "npm-run-all -s qa:forms qa:buttons:topbar",
    "tsconfig:merge": "node tools/apply-tsconfig-patch.mjs --in tsconfig.json --patch tsconfig.patch.json --out tsconfig.json",
    "package:merge": "node tools/merge-package.mjs",
    "health:local": "node tools/healthcheck.js",
    "check:fe-bd": "node tools/qa/check-map-fe-bd.js",
    "check:forms": "node tools/qa/check-forms.js",
    "check:buttons": "node tools/qa/check-buttons.js",
    "check:tables": "node tools/qa/check-tables.js",
    "check:meili": "node tools/qa/integrations/check-meili.js",
    "check:tesseract": "node tools/qa/integrations/check-tesseract.js",
    "check:ollama": "node tools/qa/integrations/check-ollama.js",
    "check:email": "node tools/qa/integrations/check-email.js",
    "check:bull": "node tools/qa/integrations/check-bullmq.js",
    "check:posthog": "node tools/qa/integrations/check-posthog.js",
    "qa:map": "npm run check:fe-bd",
    "qa:all": "npm-run-all -s qa:map qa:ui qa:integrations",
    "mocks:start": "node tools/integrations/mock-services.mjs",
    "mocks:start:bg": "node tools/integrations/mock-services.mjs &",
    "mocks:stop": "sh -c 'lsof -ti tcp:7700 | xargs kill -9 || true; lsof -ti tcp:11434 | xargs kill -9 || true; lsof -ti tcp:8025 | xargs kill -9 || true; lsof -ti tcp:9900 | xargs kill -9 || true'",
    "pm2:start": "pm2 start ecosystem.config.cjs --only icarus-frontend && pm2 save",
    "start:pm2": "pm2 start ecosystem.config.cjs --only icarus-frontend && pm2 save",
    "pm2:restart": "pm2 restart icarus-frontend",
    "pm2:stop": "pm2 stop icarus-frontend",
    "pm2:logs": "pm2 logs icarus-frontend --lines 200",
    "start": "npm run preview",
    "bench:vector": "tsx tools/bench/vector-compare.ts",
    "health:vector": "tsx tools/health/vector-endpoints.ts",
    "qa:manual": "tsx tools/audit/manual-coverage.ts",
    "audit:all": "bash scripts/audit-all.sh",
    "setup:agents": "node setup-icarus-agents.cjs",
    "audit:full": "node .cursor/scripts/orchestrator.js",
    "audit:agent": "node .cursor/scripts/run-agent.js",
    "report:generate": "node .cursor/scripts/generate-report.js",
    "logs:agent": "node .cursor/scripts/view-logs.js",
    "locks:clean": "node .cursor/scripts/clean-locks.js",
    "status:all": "node .cursor/scripts/view-status.js",
    "report:view": "node .cursor/scripts/report-view.js",
    "check:dependencies": "node .cursor/scripts/check-dependencies.js",
    "reset:all": "node .cursor/scripts/reset-all.js",
    "reset:agent": "node .cursor/scripts/reset-agent.js",
    "issues:critical": "node .cursor/scripts/issues-critical.js",
    "deploy:vercel": "node .cursor/scripts/deploy-vercel.js"
  },
