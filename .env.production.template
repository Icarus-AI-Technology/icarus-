# Variáveis de Ambiente - ICARUS v5.0 PRODUÇÃO

# ============================================
# SUPABASE (✅ Configurado)
# ============================================
VITE_SUPABASE_URL=https://your_supabase_project.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key_here

# Service Role (Backend/Migrations)
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# ============================================
# APIs EXTERNAS (Configurar se necessário)
# ============================================

# SEFAZ (NF-e)
VITE_SEFAZ_API_URL=https://nfe.sefaz.rs.gov.br/ws
VITE_SEFAZ_CERTIFICADO_PATH=/path/to/certificado.pfx
VITE_SEFAZ_CERTIFICADO_PASSWORD=your_password

# ANVISA
VITE_ANVISA_API_URL=https://consultas.anvisa.gov.br/api
VITE_ANVISA_API_KEY=your_anvisa_key

# CFM (Conselho Federal de Medicina)
VITE_CFM_API_URL=https://portal.cfm.org.br/api

# Receita Federal
VITE_RECEITA_FEDERAL_API_URL=https://www.receitaws.com.br/v1

# ViaCEP
VITE_VIA_CEP_API_URL=https://viacep.com.br/ws

# ============================================
# MICROSOFT 365 (Opcional)
# ============================================
VITE_MS365_CLIENT_ID=your_client_id
VITE_MS365_TENANT_ID=your_tenant_id
VITE_MS365_REDIRECT_URI=https://your-domain.vercel.app/auth/callback

# ============================================
# OPENAI / GPT RESEARCHER (Opcional)
# ============================================
VITE_OPENAI_API_KEY=your_openai_key
VITE_GPT_RESEARCHER_URL=http://localhost:8000

# ============================================
# MONITORING (Recomendado)
# ============================================
VITE_SENTRY_DSN=your_sentry_dsn
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# ============================================
# ENVIRONMENT
# ============================================
NODE_ENV=production
VITE_APP_ENV=production
